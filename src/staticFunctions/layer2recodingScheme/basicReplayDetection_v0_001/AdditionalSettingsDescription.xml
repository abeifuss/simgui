<?xml version="1.0" encoding="UTF-8"?>
<specificSettings>
	<name>Basi Replay Detection specific settings</name>
	<settingsGroups>
		<settingGroup>
			<name>Connection Handler</name>
			<settings>
				<singleSetting>
					<name>REPLAY_DB_CAPACITY</name>
					<location type="textcontent">/staticFunction/replayDbCapacity</location>
					<content>
						<int preset="5000" min="10" max="100000" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>DB_LOAD_FACTOR</name>
					<location type="textcontent">/staticFunction/dbLoadFactor</location>
					<content>
						<float preset="0.75" min="0" max="1"/>
					</content>
				</singleSetting>
			</settings>			
		</settingGroup>
	</settingsGroups>
</specificSettings>
