<?xml version="1.0" encoding="UTF-8"?>
<specificSettings>
	<name>Settings for waitForReply</name>
	<settingsGroups>
		<settingGroup>
			<name>Specific Settings</name>
			<settings>
				<singleSetting>
					<name>WAIT_FOR_REPLY_REQUEST_QUEUE_SIZE</name>
					<location type="textcontent">/plugIn/waitForReplyRequestQueueSize</location>
					<content>
						<int preset="1" min="1" max="100"/>
					</content>
				</singleSetting>
				<singleSetting>
					<name>WAIT_FOR_REPLY_REPLY_QUEUE_SIZE</name>
					<location type="textcontent">/plugIn/waitForReplyReplyQueueSize</location>
					<content>
						<int preset="1" min="1" max="100"/>
					</content>
				</singleSetting>
				<singleSetting>
					<name>WAIT_FOR_REPLY_DELAY</name>
					<location type="textcontent">/plugIn/waitForReplyDelay</location>
					<content>
						<int preset="500" min="1" max="100000"/>
					</content>
				</singleSetting>
			</settings>
		</settingGroup>
	</settingsGroups>
</specificSettings>
