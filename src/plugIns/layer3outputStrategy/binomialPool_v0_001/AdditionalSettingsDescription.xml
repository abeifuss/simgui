<?xml version="1.0" encoding="UTF-8"?>
<specificSettings>
	<name>Settings for binomialPool</name>
	<settingsGroups>
		<settingGroup>
			<name>Specific Settings</name>
			<settings>
				<singleSetting>
					<name>BINOMIAL_POOL_DEFAULT_POOL_SIZE</name>
					<location type="textcontent">/plugIn/binomialPoolDefaultPoolSize</location>
					<content>
						<int preset="100" min="1" max="1000" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_POOL_SENDING_RATE (ms)</name>
					<location type="textcontent">/plugIn/binomiaPoolSendinRate</location>
					<content>
						<int preset="1000" min="1" max="100" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_POOL_MAX_OUTPUT_FRACTION (ms)</name>
					<location type="textcontent">/plugIn/binomialPoolMaxOutputFraction</location>
					<content>
						<float preset="0.7" min="0" max="1" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_POOL_MEAN (ms)</name>
					<location type="textcontent">/plugIn/binomialPoolMean</location>
					<content>
						<int preset="100" min="1" max="1000" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_POOL_STDDEV (ms)</name>
					<location type="textcontent">/plugIn/binomialPoolStddev</location>
					<content>
						<int preset="30" min="1" max="100" />
					</content>
				</singleSetting>
			</settings>			
		</settingGroup>
	</settingsGroups>
</specificSettings>
