<?xml version="1.0" encoding="UTF-8"?>
<specificSettings>
	<name>Settings for logarithmicPool</name>
	<settingsGroups>
		<settingGroup>
			<name>Specific Settings</name>
			<settings>
				<singleSetting>
					<name>LOG_POOL_DEFAULT_POOL_SIZE</name>
					<location type="textcontent">/plugIn/logPoolDefaultPoolSize</location>
					<content>
						<int preset="10" min="1" max="1000" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>LOG_POOL_SEND_INTERVAL</name>
					<location type="textcontent">/plugIn/logPoolSendInterval</location>
					<content>
						<int preset="1000" min="1" max="10000" />
					</content>
				</singleSetting>
			</settings>
		</settingGroup>
	</settingsGroups>
</specificSettings>