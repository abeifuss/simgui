<?xml version="1.0" encoding="UTF-8"?>
<specificSettings>
	<name>Settings for binomialPlusPool</name>
	<settingsGroups>
		<settingGroup>
			<name>Specific Settings</name>
			<settings>
				<singleSetting>
					<name>BINOMIAL_PLUS_POOL_DEFAULT_POOL_SIZE</name>
					<location type="textcontent">/plugIn/binomialPlusPoolDefaultPoolSize</location>
					<content>
						<int preset="100" min="1" max="1000" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_PLUS_POOL_SEND_INTERVAL (ms)</name>
					<location type="textcontent">/plugIn/binomiaPlusPoolSendInterval</location>
					<content>
						<int preset="1000" min="1" max="100" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_PLUS_POOL_MIN_POOL_SIZE (ms)</name>
					<location type="textcontent">/plugIn/binomialPlusPoolMinPoolSize</location>
					<content>
						<int preset="10" min="1" max="100" />
					</content>
				</singleSetting>
				<singleSetting>
					<name>BINOMIAL_PLUS_POOL_K (ms)</name>
					<location type="textcontent">/plugIn/binomialPlusPoolK</location>
					<content>
						<float preset="0.01" min="0" max="1" />
					</content>
				</singleSetting>
			</settings>			
		</settingGroup>
	</settingsGroups>
</specificSettings>
