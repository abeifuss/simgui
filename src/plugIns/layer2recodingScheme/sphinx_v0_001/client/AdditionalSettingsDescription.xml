<?xml version="1.0" encoding="UTF-8"?>
<specificSettings>
	<name>Sphinx specific Settings</name>
	<settingsGroups>
		<settingGroup>
			<settings>
				<singleSetting>
					<name>PRNG_ALGORITHM</name>
					<location type="textcontent">/plugIn/prngAlgorithm</location>
					<content>
						<string preset="SHA1PRNG" pattern="[A-Za-z0-9_]+" />
					</content>
				</singleSetting> 
			</settings>
		</settingGroup>
	</settingsGroups>
</specificSettings>
